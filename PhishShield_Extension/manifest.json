{
  "manifest_version": 3,
  "name": "PhishShield",
  "version": "1.0",
  "description": "Phishing detection using ML",

  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],

  "permissions": [
    "tabs",
    "storage"
  ],

  "host_permissions": [
  "http://localhost:8000/*",
  "http://127.0.0.1:8000/*"
]

}
